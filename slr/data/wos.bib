@article{ WOS:001436428000023,
Author = {Hedegaard, Michael and Hrytsenko, Larysa and Derkach, Liliia},
Title = {FOREIGN DIRECT INVESTMENT IN UKRAINE: A PATHWAY TO SUSTAINABILITY},
Journal = {FINANCIAL AND CREDIT ACTIVITY-PROBLEMS OF THEORY AND PRACTICE},
