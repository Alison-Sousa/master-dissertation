@article{ WOS:001409236300004,
Author = {Cheng, Q.},
Title = {MANAGING OUTWARD FOREIGN DIRECT INVESTMENT RISKS OF REAL ESTATE PROJECTS
   BY CHINESE DEVELOPERS IN MALAYSIA},
Journal = {POLISH JOURNAL OF MANAGEMENT STUDIES},
Year = {2024},
Volume = {30},
Number = {2},
Pages = {57-74},
Abstract = {This study investigates the risks associated with Chinese developers'
   outward foreign direct investment (OFDI) in the Malaysian real estate
   sector. Adopting a mixed- methods approach integrating quantitative
   surveys and qualitative interviews, the study comprehensively identifies
   and analyzes various political, legal, economic, social, environmental,
   and home country risks. Key findings reveal that the COVID-19 pandemic,
   China's foreign exchange control policies, and political instability in
   Malaysia are the most critical risk factors. As a novel contribution,
   the study develops an adaptive risk management framework tailored
   specifically for international real estate projects, with strategies
   focused on active political engagement, financial diversification, legal
   adaptation, cultural integration, and a focus on environmental
   sustainability. This framework aims to enhance the resilience and
   profitability of Chinese developers in the Malaysian real estate sector,
   offering actionable insights for developers, investors, and
   policymakers.},
