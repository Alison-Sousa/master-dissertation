@article{ WOS:001538971900019,
Author = {Vashakmadze, Irina and Jabnidze, Nato and Tavadze, Irine and
   Robitashvili, Natalia},
Title = {VOLATILITY IN GEORGIA, ITS INFLUENCE ON THE INVESTMENT DECISIONS OF
   MULTINATIONAL ENTERPRISES AND THE IMPLICATIONS FOR FOREIGN DIRECT
   INVESTMENT DYNAMICS IN THE COUNTRY},
Journal = {FINANCIAL AND CREDIT ACTIVITY-PROBLEMS OF THEORY AND PRACTICE},
Year = {2025},
Volume = {3},
