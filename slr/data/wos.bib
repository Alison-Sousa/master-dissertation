@article{ WOS:001162790600001,
Author = {Borah, Prasad Siba and Dogbe, Courage Simon Kofi and Marwa, Nyankomo},
Title = {Generation Z's green purchase behavior: Do green consumer knowledge,
   consumer social responsibility, green advertising, and green consumer
   trust matter for sustainable development?},
